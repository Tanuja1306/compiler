// SCOPE TEST PROGRAM
program scope_test is
    global integer my_global_int;
    integer result;

    procedure increment_global(integer result out)
    begin
        result := my_global_int + 1;
        return;
    end procedure;

begin
    my_global_int := 9;
    result := 0;

    increment_global(result);
    putString("Expect 10");
    putInteger(result);

    return;

end program
